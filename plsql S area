CREATE TABLE areas(
     radius INT,
    area DECIMAL
    );
DELIMITER $$

CREATE PROCEDURE circle_area_calc()
BEGIN
    DECLARE r INT DEFAULT 5;
    DECLARE a DECIMAL(10,2);
    DECLARE pi DECIMAL(10,5) DEFAULT 3.14159;

    WHILE r <= 9 DO
        SET a = pi * r * r;
        INSERT INTO areas VALUES (r, a);
        SET r = r + 1;
    END WHILE;
END $$

DELIMITER ;

CALL
 
SELECT
